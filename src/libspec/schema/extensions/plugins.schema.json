    "DiscoveryMechanismType": {
      "description": "How plugins are discovered and loaded.\n\n- entry_points: Python package entry points (setuptools)\n- namespace_packages: Namespace package scanning\n- directory_scan: Filesystem directory scanning\n- config_file: Configuration file listing\n- explicit: Explicit programmatic registration\n- decorator: Decorator-based registration",
      "enum": [
        "entry_points",
        "namespace_packages",
        "directory_scan",
        "config_file",
        "explicit",
        "decorator"
      ],
      "title": "DiscoveryMechanismType",
      "type": "string"
